<table id= "WeeklyTable" class = "display">

    <thead>
            <th colspan="2">
                WEEKLY PERFORMANCE
            </th>
            
           
    </thead>
    <tbody>
            <tr>
                <td align="left" style ="text-align:left">
               Working AAs Per Week
                
                </td>
                <td>
                @Model.WorkingAAPerWeek
                </td>
             </tr>
             <tr>
               <td align="left" style ="text-align:left">
                 Appointments Per Week
                
