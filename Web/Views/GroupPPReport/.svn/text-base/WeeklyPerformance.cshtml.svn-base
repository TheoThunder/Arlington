                </td>
             </tr>
             <tr>
                <td align="left" style ="text-align:left">
                  Good Appointments Per Week
                
                </td>
                <td>
                     @Model.GoodAppointmentsPerWeek

                </td>
             </tr>
             <tr>
                <td align="left" style ="text-align:left">
                    Closes Per Week
                
                </td>
