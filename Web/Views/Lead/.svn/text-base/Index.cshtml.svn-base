@model IEnumerable<Data.Domain.Lead>

@{
    ViewBag.Title = "Index";
}
<link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("../../Content/demo_page.css")" rel="Stylesheet" type="text/css" />
<link href="@Url.Content("../../Content/demo_table.css")" rel="Stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/demo_table_jui.css")" rel="stylesheet" type="text/css" />
<script src = "@Url.Content("~/Scripts/jquery.dataTables.js")" type="text/javascript"></script>
<script src = "@Url.Content("~/Scripts/jquery.jeditable.js")" type="text/javascript"></script>


<script type="text/javascript">
    $(document).ready(function () {
        $('#LeadsTable').dataTable({
            "sPaginationType": "full_numbers"
        });
    });
</script>

<div class="container">
<div class="header">
<div class="logo"><img border="0" src="../../Content/images/banner.png" alt="alt"/></div>  
    <div class="hometab"><a href="/home/index">Home</a></div>
			<div class="implea"><a href="/importlead/index">Import Leads</a></div>
         	<div class="leadque"><a href="/leadqueue/index">Leads Queue</a></div>
            <div class="apptque"><a href="/appointmentqueue/index">Appt Queue</a></div>
            <div class="dashb"><a href="@Url.Action("Index", "DashBoard")">Dashboard</a></div>  
             
            <div class="logout"><a href="/auth/login" style="color:White">Name</a>
            <ul class="logoutlist" >  
   				   @* <li>@Html.ActionLink("Profile", "Index2", "User", new { id = Model.user.UserId }, null)</li>  *@
             		<li><a href="/auth/login">Logout</a></li> 
   				  </ul>
             </div>

<div class="content">

<div style="position:relative; margin-left:auto; margin-right:auto; width: 65%;">
<h2>Profiles</h2>
@*<div class="newemployee">
    @Html.ActionLink("Create New", "Create")
</div>*@
<table id="LeadsTable" class="display" style= "border:1px solid #000; ">
<thead>
    <tr>
        
        <th>
            Company Name
        </th>
        <th>
            City
        </th>
        <th>
            State
        </th>
        <th>
            Zip Code
        </th>
        <th>
            Status
        </th>
        <th># of Accounts
        </th>
        <th>
        </th>
        <th>
        </th>
        <th></th>
        <th></th>
    </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
    
    <tr>
     
        <td>
            @item.CompanyName
        </td>
        <td>
            @item.City
        </td>
        <td>
            @item.State
        </td>
        <td>
            @item.ZipCode
        </td>
        <td>
            @item.Status
        </td>
        <td>
            @item.accounts
        </td>
        <td>
        </td>
        <td>
        </td>
           <td>
           @* @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ })*@
            </td>
            <td>
           @* @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })*@
        </td>
    </tr>
    
}</tbody>

</table>
</div>
</div>
</div>