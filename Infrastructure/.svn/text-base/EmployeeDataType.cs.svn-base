using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Infrastructure
{
    public class EmployeeDataType
    {
        String _employee;
        Int32 _good;
        Int32 _set;
        Int32 _closes;
        Int32 _todayt;
        String _colorg;
        String _colors;
        String _colorc;
        String _colort;
        Int32 _team;

        public EmployeeDataType(string employee, int good, string colorg, int set, string colors, int closes, string colorc, int todayt, string colort, int team)
        {
            Employee = employee;
            Good = good;
            Set = set;
            Closes = closes;
            TodayT = todayt;
            ColorG = colorg;
            ColorS = colors;
            ColorC = colorc;
            ColorT = colort;
            Team = team;
        }

        public String Employee
        {
            get
            {
                return _employee;
            }
            set
            {
                _employee = value;
            }
        }

        public Int32 Good
        {
            get
            {
                return _good;
            }
            set
            {
                _good = value;
            }
        }

        public Int32 Set
        {
            get
            {
                return _set;
            }
            set
            {
                _set = value;
            }
        }

        public Int32 Closes
        {
            get
            {
                return _closes;
            }
            set
            {
                _closes = value;
            }
        }

        public Int32 TodayT
        {
            get
            {
                return _todayt;
            }
            set
            {
                _todayt = value;
            }
        }

        public String ColorG
        {
            get
            {
                return _colorg;
            }
            set
            {
                _colorg = value;
            }
        }

        public String ColorS
        {
            get
            {
                return _colors;
            }
            set
            {
                _colors = value;
            }
        }

        public String ColorC
        {
            get
            {
                return _colorc;
            }
            set
            {
                _colorc = value;
            }
        }

        public String ColorT
        {
            get
            {
                return _colort;
            }
            set
            {
                _colort = value;
            }
        }
        public int Team
        {
            get
            {
                return _team;
            }
            set
            {
                _team = value;
            }
        }
        
    }
}
